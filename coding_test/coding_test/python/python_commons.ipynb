   "source": [
    "import string\n",
    "\n",
    "alpha_chars = {i:ord(i) for i in string.ascii_uppercase}\n",
    "search_code = 80\n",
    "\n",
    "for character, code in alpha_chars.items():\n",
    "    if code == search_code:\n",
    "        print(character)\n"
   ]
